<h1><strong>Lists</strong></h1>


<% @lists.each do |list| %>
  <div class = "card border-succes mb-3">
  <p class="card-title"><%= list.name %></p>
   <%= link_to "See bookmarks", lists_path(list.id), class: "btn btn-outline-primary"%>
  </div>
<% end %>


<%= link_to "Create new list", lists_new_path, class: "btn btn-outline-primary" %>
